                                                    <path d='M64.143,172.273v64.348c84.881,0,153.938,69.056,153.938,153.939h64.348    C282.429,270.196,184.507,172.273,64.143,172.273z' style='fill:#FFFFFF;'/>
                                                    <circle cx='109.833' cy='346.26' r='46.088' style='fill:#FFFFFF;'/>
                                                </g>
                                            </g>
                                            <g/>
                                            <g/>
                                            <g/>
                                            <g/>
